{
  "expo": {
    "name": "The Power Of Heart",
    "slug": "poh",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f0f0f0"
    },
    "packagerOpts": {
      "sourceExts": [
        "js",
        "jsx",
        "scss",
        "sass"
      ],
      "transformer": "node_modules/react-native-sass-transformer/index.js"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}